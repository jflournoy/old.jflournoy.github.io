	date = {2006-06-21},
	pmid = {16793895},
	keywords = {Adolescent, Adult, Age Factors, Brain Mapping, Child, Choice Behavior, Female, Frontal Lobe, Humans, Image Processing, Computer-Assisted, Magnetic Resonance Imaging, Male, Neural Pathways, Nucleus accumbens, Oxygen, Reaction Time, Reward, Risk-taking, development, Linear Models, accumbens, orbital frontal cortex},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/M6W3DEXR/Galvan et al. - 2006 - Earlier Development of the Accumbens Relative to O.pdf:application/pdf;Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FJCRI4I9/Galvan et al. - 2006 - Earlier Development of the Accumbens Relative to O.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/ZMMWQN3J/6885.html:text/html;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WIQ3QJE7/6885.html:text/html}
