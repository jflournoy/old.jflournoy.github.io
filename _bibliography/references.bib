@article{slovic_rational_2002,
	title = {Rational actors or rational fools: implications of the affect heuristic for behavioral economics},
	volume = {31},
	issn = {1053-5357},
	url = {http://www.sciencedirect.com/science/article/pii/S1053535702001749},
	doi = {10.1016/S1053-5357(02)00174-9},
	shorttitle = {Rational actors or rational fools},
	abstract = {This paper describes two fundamental modes of thinking. The experiential mode, is intuitive, automatic, natural, and based upon images to which positive and negative affective feelings have been attached through learning and experience. The other mode is analytic, deliberative, and reason based. I describe recent empirical research illuminating “the affect heuristic” wherein people rapidly consult their affective feelings, when making judgments and decisions. This heuristic enables us to be rational actors in many situations. It works beautifully when experience enables us to anticipate accurately how we will like or dislike the consequences of our decisions. However, it fails miserably when the consequences turn out to be much different than we anticipated. In the latter circumstances, the rational actor may well become the rational fool.},
	pages = {329--342},
	number = {4},
	journaltitle = {The Journal of Socio-Economics},
	shortjournal = {The Journal of Socio-Economics},
	author = {Slovic, Paul and Finucane, Melissa and Peters, Ellen and {MacGregor}, Donald G},
	urldate = {2013-08-01},
	date = {2002},
	keywords = {Behavioral economics, Rational actors, The affect heuristic},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/6F4MDVSP/Slovic et al. - 2002 - Rational actors or rational fools implications of.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JNX8NGMB/S1053535702001749.html:text/html}
}

@article{izuma_social_2013,
	title = {Social Manipulation of Preference in the Human Brain},
	volume = {78},
	issn = {0896-6273},
	url = {http://www.cell.com/neuron/abstract/S0896-6273(13)00270-5},
	doi = {10.1016/j.neuron.2013.03.023},
	abstract = {Our preferences are influenced by what other people like, but depend critically on how we feel about those people, a classical psychological effect called “cognitive balance.” Here, we manipulated preferences for goods by telling participants the preferences of strongly liked or disliked groups of other people. Participants’ preferences converged to those of the liked group, but diverged from the disliked group. Activation of dorsomedial prefrontal cortex ({dmPFC}) tracked the discrepancy between one’s own preference and its social ideal and was associated with subsequent preference change (toward the liked and away from the disliked group), even several months later. A follow-up study found overlapping activation in this same region of {dmPFC} with negative monetary outcomes, but no overlap with nearby activations induced by response conflict. A single social encounter can thus result in long-lasting preference change, a mechanism that recruits {dmPFC} and that may reflect the aversive nature of cognitive imbalance.},
	pages = {563--573},
	number = {3},
	journaltitle = {Neuron},
	author = {Izuma, Keise and Adolphs, Ralph},
	urldate = {2013-11-01},
	date = {2013-05-08},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/2CFTZZ2K/Izuma and Adolphs - 2013 - Social Manipulation of Preference in the Human Bra.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/5FDWFG3H/S0896-6273(13)00270-5.html:text/html}
}

@article{de_braek_identification_2009,
	title = {The Identification of Attention Complaints in the General Population and Their Effect on Quality of Life},
	url = {http://www.ncbi.nlm.nih.gov.laneproxy.stanford.edu/pubmed/19794137},
	doi = {10.1177/1087054709347260},
	abstract = {Objective: To provide more insight into subjective attention complaints in a healthy adult and elderly population and how these affect Quality of Life ({QoL}). Method: A group of 1,550 healthy Dutch participants complete a postal questionnaire including items from the Maastricht Attention and Memory Checklist ({MAC}).The impact of attention complaints on {QoL} is investigated in a subsample of 499 participants. Results: Factor analysis (N =1,550) reveals two factors:Attention and Memory.Attention complaints are related to depressed mood, anxiety, vitality, and sleep problems that can have serious consequences for daily life functioning and {QoL} (n =499). Memory complaints are related to other aspects of health, such as pain and changes in health. Conclusion: Attention complaints in the healthy population are common and related to depression, anxiety, and sleep and several aspects of {QoL}, such as problems with social functioning, emotional problems, and vitality.},
	journaltitle = {Journal of Attention Disorders},
	shortjournal = {J Atten Disord},
