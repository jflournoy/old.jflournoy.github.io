}

@article{luke_differentiated_2013,
	title = {Differentiated associations between childhood maltreatment experiences and social understanding: A meta-analysis and systematic review},
	volume = {33},
	issn = {0273-2297},
	url = {http://www.sciencedirect.com/science/article/pii/S0273229712000512},
	doi = {10.1016/j.dr.2012.10.001},
	shorttitle = {Differentiated associations between childhood maltreatment experiences and social understanding},
	abstract = {The extreme parenting experiences encountered by children who are physically abused or neglected place them at increased risk for impaired socio-emotional development. There is growing evidence that maltreated children may apprehend interpersonal encounters in different ways from children without such traumatic histories. This systematic review examines the links between childhood physical abuse and neglect and various constituent skills of social understanding (including emotion recognition and understanding, perspective taking, false belief understanding, and attributional biases) in 51 empirical studies. The review incorporates a meta-analysis of 19 studies on emotion recognition and understanding in this population. This showed an overall negative effect of maltreatment, but moderation analyses revealed that significantly stronger effects were found for measures of emotion understanding rather than recognition, and for younger rather than older age groups. The broader review also reveals a complex and differentiated profile of social understanding among maltreated children. Directions for future research that addresses individual differences in children’s experiences, both within and outside the maltreatment context, are discussed.},
	pages = {1--28},
	number = {1},
	journaltitle = {Developmental Review},
	shortjournal = {Developmental Review},
	author = {Luke, Nikki and Banerjee, Robin},
	urldate = {2013-03-18},
	date = {2013-03},
	keywords = {Emotion recognition, Maltreatment, Meta-analysis, Neglect, Physical abuse, Social understanding, Systematic review, Theory of Mind},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/2GKUAHPC/Luke and Banerjee - 2013 - Differentiated associations between childhood malt.pdf:application/pdf;ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FDFWRSZA/Luke and Banerjee - 2013 - Differentiated associations between childhood malt.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/492FE76J/S0273229712000512.html:text/html;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/PTZIFXFJ/S0273229712000512.html:text/html}
