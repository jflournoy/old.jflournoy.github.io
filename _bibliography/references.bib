	pmcid = {PMC2574721},
	keywords = {Adolescent, Anxiety, Attention, Bias (Epidemiology), Child, Female, Humans, Image Processing, Computer-Assisted, Magnetic Resonance Imaging, Male, Oxygen, Pattern Recognition, Visual, Photic Stimulation, Prefrontal Cortex, Reaction Time, anger, facial expression}
