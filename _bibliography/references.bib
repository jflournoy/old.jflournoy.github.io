	langid = {english},
	keywords = {Artificiality, C70, C91, D63, D64, Economic Theory, Experiments, Methodology, Methodology and the History of Economic Thought, Microeconomics, Operations Research/Decision Theory, Public Finance \& Economics, altruism},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/VHX3UM3N/Bardsley - 2008 - Dictator game giving altruism or artefact.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/G9NJZQJ7/s10683-007-9172-2.html:text/html}
}

@article{moeller_you_2011,
