	pmid = {8000446},
	keywords = {Analysis of Variance, Androstanols, Animals, Antidepressive Agents, Binding, Competitive, Citalopram, Corticosterone, Desipramine, Hippocampus, Male, Maprotiline, Mianserin, Norepinephrine, Radioimmunoassay, Rats, Rats, Wistar, Receptors, Glucocorticoid, Receptors, Mineralocorticoid, Receptors, Steroid, Serotonin Antagonists}
