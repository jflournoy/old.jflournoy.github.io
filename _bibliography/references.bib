	keywords = {Communities, Internet, Pragmatics, Psychology, Wizard-of-Oz, affect process, affect sensing and analysis, affective dialog system, affective profiles, affective states, chatting enjoyment rating, computer games, computer mediated communication, dialog act class usage, dialog coherence, emotion social sharing, emotional connection, fine-grained online communication scenarios, human computer interaction, human-computer interaction, interaction pattern analysis, physiology, realism, self-reported emotional changes, social process, structuring affective interactions, text analysis, textual expressions, user expressions, virtual reality, word categories},
	file = {IEEE Xplore Abstract Record:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/IQ23P5UR/icp.html:text/html}
