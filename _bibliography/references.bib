	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/MEGT6TF7/Levine et al. - 2011 - Sender Demeanor Individual Differences in Sender .pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/5967W2XE/full.html:text/html}
}

