	urldate = {2012-09-27},
	date = {2011-12},
	keywords = {Affect recognition, Disorganization symptoms, Metacognition, Negative symptoms, Neurocognition, Schizophrenia, Self, Theory of Mind, Trauma, depression},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/A6W6Z2UT/Lysaker et al. - 2011 - Deficits in the ability to recognize one’s own aff.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/W5KVI5H2/S1053810010002771.html:text/html}
