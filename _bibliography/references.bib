@article{konen_two_2008,
	title = {Two hierarchically organized neural systems for object information in human visual cortex},
	volume = {11},
	rights = {© 2008 Nature Publishing Group},
	issn = {1097-6256},
	url = {http://www.nature.com/neuro/journal/v11/n2/full/nn2036.html},
	doi = {10.1038/nn2036},
	abstract = {The primate visual system is broadly organized into two segregated processing pathways, a ventral stream for object vision and a dorsal stream for space vision. Here, evidence from functional brain imaging in humans demonstrates that object representations are not confined to the ventral pathway, but can also be found in several areas along the dorsal pathway. In both streams, areas at intermediate processing stages in extrastriate cortex (V4, V3A, {MT} and V7) showed object-selective but viewpoint- and size-specific responses. In contrast, higher-order areas in lateral occipital and posterior parietal cortex ({LOC}, {IPS}1 and {IPS}2) responded selectively to objects independent of image transformations. Contrary to the two-pathways hypothesis, our findings indicate that basic object information related to shape, size and viewpoint may be represented similarly in two parallel and hierarchically organized neural systems in the ventral and dorsal visual pathways.},
	pages = {224--231},
	number = {2},
	journaltitle = {Nature Neuroscience},
	shortjournal = {Nat Neurosci},
	author = {Konen, Christina S. and Kastner, Sabine},
	urldate = {2013-05-11},
