	url = {http://link.springer.com/article/10.1023/A%3A1010490917575},
	doi = {10.1023/A:1010490917575},
	shorttitle = {What's Different in Second-Language Processing?},
	abstract = {German sentences which were either correct, contained a selectional restriction violation, or a word category violation were presented auditorily to 16 native speakers of German (L1 group) and to 16 native speakers of Russian, who had learned German after the age of 10 (L2 group). Semantic violations elicited an N400 effect for both groups, but with a reduced amplitude and a longer peak latency in the L2 group. Compared to correct sentences, sentences with a phrase structure violation elicited an early anterior negativity followed by a broad centro-parietal positivity in native speakers. By contrast, there was no differential modulation of the early anterior negativity in the L2 group. A late positivity was also elicited in the second language learners, but it was slightly delayed compared to that shown by native speakers. This pattern is discussed in terms of different degrees of automaticity with respect to the subprocesses involved in sentence comprehension.},
	pages = {251--266},
	number = {3},
	journaltitle = {Journal of Psycholinguistic Research},
	shortjournal = {J Psycholinguist Res},
	author = {Hahne, Anja},
	urldate = {2013-06-13},
	date = {2001-05-01},
	langid = {english},
	keywords = {Cognitive Psychology, {ERPs}, Psycholinguistics, second language learning, semantics, sentence processing, syntax},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/6EXBE4T6/Hahne - 2001 - What's Different in Second-Language Processing Ev.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/D6QDRXQJ/A1010490917575.html:text/html}
