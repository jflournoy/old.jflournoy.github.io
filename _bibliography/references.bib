@article{jayawickreme_virtuous_2014,
	title = {Virtuous states and virtuous traits: How the empirical evidence regarding the existance of broad traits saves virtue ethics from the situationist critique},
	issn = {1477-8785, 1741-3192},
	url = {http://tre.sagepub.com/content/early/2014/08/05/1477878514545206},
	doi = {10.1177/1477878514545206},
	shorttitle = {Virtuous states and virtuous traits},
	abstract = {A major objection to the study of virtue asserts that the empirical psychological evidence implies traits have little meaningful impact on behavior, as slight changes in situational characteristics appear to lead to large changes in virtuous behavior. We argue in response that the critical evidence is not these effects of situations observed in social psychological experiments, but evidence of stable individual differences obtained from correlations of individual’s behaviors across multiple contexts. The totality of the empirical evidence is shown to support this claim: broad traits are real, prominent, and consequential, and these traits, conceptualized as density distributions of personality states, exhibit remarkable consistency. In short, the evidence in favor of individual differences is empirically solid, and the study of ethics focused on virtue is not threatened by psychological research.},
	pages = {1477878514545206},
	journaltitle = {Theory and Research in Education},
	shortjournal = {Theory and Research in Education},
	author = {Jayawickreme, Eranda and Meindl, Peter and Helzer, Erik G. and Furr, R. Michael and Fleeson, William},
	urldate = {2015-05-15},
	date = {2014-08-06},
	langid = {english},
	keywords = {traits, density distributions of personality states, situationism, virtue theory},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/F33U39FG/Jayawickreme et al. - 2014 - Virtuous states and virtuous traits How the empir.pdf:application/pdf;Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/R6IUDERG/Jayawickreme et al. - 2014 - Virtuous states and virtuous traits How the empir.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FJTQ75JK/1477878514545206.html:text/html;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/GGFERASA/1477878514545206.html:text/html}
