	file = {McGann-2014-Enacting_a_social_ecology.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/9A34HPU9/McGann-2014-Enacting_a_social_ecology.pdf:application/pdf}
}

@article{jennings_publication_2011,
	title = {Publication Bias in Neuroimaging Research: Implications for Meta-Analyses},
	volume = {10},
	issn = {1539-2791, 1559-0089},
	url = {http://link.springer.com/article/10.1007/s12021-011-9125-y},
	doi = {10.1007/s12021-011-9125-y},
	shorttitle = {Publication Bias in Neuroimaging Research},
	abstract = {Neuroimaging and the neurosciences have made notable advances in sharing activation results through detailed databases, making meta-analysis of the published research faster and easier. However, the effect of publication bias in these fields has not been previously addressed or accounted for in the developed meta-analytic methods. In this article, we examine publication bias in functional magnetic resonance imaging ({fMRI}) for tasks involving working memory in the frontal lobes (Brodmann Areas 4, 6, 8, 9, 10, 37, 45, 46, and 47). Seventy-four studies were selected from the literature and the effect of publication bias was examined using a number of regression-based techniques. Pearson’s r correlation coefficient and Cohen’s d effect size estimates were computed for the activation in each study and compared to the study sample size using Egger’s regression, Macaskill’s regression, and the ‘Trim and Fill’ method. Evidence for publication bias was identified in this body of literature (p {\textless} 0.01 for each test), generally, though was neither task- nor sub-region-dependent. While we focused our analysis on this subgroup of brain mapping studies, we believe our findings generalize to the brain imaging literature as a whole and databases seeking to curate their collective results. While neuroimaging databases of summary effects are of enormous value to the community, the potential publication bias should be considered when performing meta-analyses based on database contents.},
	pages = {67--80},
	number = {1},
	journaltitle = {Neuroinformatics},
	shortjournal = {Neuroinform},
	author = {Jennings, Robin G. and Horn, John D. Van},
	urldate = {2016-02-18},
	date = {2011-06-04},
	langid = {english},
	keywords = {Bioinformatics, Brain imaging, Computer Appl. in Life Sciences, Meta-analysis, Neurosciences, {fMRI}, publication bias, databases, Computational Biology/Bioinformatics, Neurology},
	file = {Jennings and Horn - 2011 - Publication Bias in Neuroimaging Research Implica.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/3MEEDW3Q/Jennings and Horn - 2011 - Publication Bias in Neuroimaging Research Implica.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/EEK5AWED/s12021-011-9125-y.html:text/html}
}

@article{gorgolewski_practical_2016,
	title = {A practical guide for improving transparency and reproducibility in neuroimaging research},
	rights = {© 2016, Published by Cold Spring Harbor Laboratory Press. This pre-print is available under a Creative Commons License (Attribution 4.0 International), {CC} {BY} 4.0, as described at http://creativecommons.org/licenses/by/4.0/},
