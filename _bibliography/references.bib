	pmid = {26581729},
	keywords = {Decision Making, diversity, economic growth, transparency, innovation},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/D78ZVBTJ/Galinsky et al. - 2015 - Maximizing the Gains and Minimizing the Pains of D.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/E5KC7EZH/742.html:text/html}
