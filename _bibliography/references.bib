	langid = {english},
	keywords = {Apostasy, Human Geography, Microeconomics, Public Health/Gesundheitswesen, Quality of Life Research, Religion, Religiosity, Sociology, {WELL}-being},
	file = {Mochon et al. - 2011 - Who Benefits from Religion.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/2U27FAS5/Mochon et al. - 2011 - Who Benefits from Religion.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/5SINIJ6F/s11205-010-9637-0.html:text/html}
