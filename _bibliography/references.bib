@article{dal_monte_voxel-based_2012,
	title = {A Voxel-Based Lesion Study on Facial Emotion Recognition After Penetrating Brain Injury},
	issn = {1749-5016, 1749-5024},
	url = {http://scan.oxfordjournals.org/content/early/2012/06/04/scan.nss041},
	doi = {10.1093/scan/nss041},
	abstract = {The ability to read emotions in the face of another person is an important social skill that can be impaired in subjects with traumatic brain injury ({TBI}). To determine the brain regions that modulate facial emotion recognition, we conducted a whole-brain analysis using a well-validated facial emotion recognition task and voxel-based lesion symptom mapping ({VLSM}) in a large sample of patients with focal penetrating {TBIs} ({pTBIs}). Our results revealed that individuals with {pTBI} performed significantly worse than normal controls in recognizing unpleasant emotions. {VLSM} mapping results showed that impairment in facial emotion recognition was due to damage in a bilateral fronto-temporo-limbic network, including medial prefrontal cortex ({PFC}), anterior cingulate cortex, left insula and temporal areas. Beside those common areas, damage to the bilateral and anterior regions of {PFC} led to impairment in recognizing unpleasant emotions, whereas bilateral posterior {PFC} and left temporal areas led to impairment in recognizing pleasant emotions. Our findings add empirical evidence that the ability to read pleasant and unpleasant emotions in other people's faces is a complex process involving not only a common network that includes bilateral fronto-temporo-limbic lobes, but also other regions depending on emotional valence.},
	journaltitle = {Social Cognitive and Affective Neuroscience},
	shortjournal = {Soc Cogn Affect Neurosci},
	author = {Dal Monte, Olga and Krueger, Frank and Solomon, Jeffrey M. and Schintu, Selene and Knutson, Kristine M. and Strenziok, Maren and Pardini, Matteo and Leopold, Anne and Raymont, Vanessa and Grafman, Jordan},
	urldate = {2012-06-07},
	date = {2012-04-11},
	langid = {english},
	keywords = {basic emotions, facial emotion recognition, labeling task, traumatic brain injury, voxel-based lesion symptom mapping},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/M5SDAK6A/Dal Monte et al. - 2012 - A Voxel-Based Lesion Study on Facial Emotion Recog.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/PETH6D5S/scan.nss041.html:text/html}
}

