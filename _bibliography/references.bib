	langid = {english},
	keywords = {Adolescence, Child and School Psychology, Clinical Psychology, Health Psychology, History of Psychology, Law and Psychology, Psychology, general, positive youth development, Short form, Confirmatory factor analysis ({CFA}), Longitudinal measurement invariance},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/7ST2GWSW/Geldhof et al. - 2014 - Longitudinal Analysis of a Very Short Measure of P.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/E5IXG7ZS/s10964-014-0093-z.html:text/html}
}

@article{hershberg_illuminating_2014,
	title = {Illuminating Trajectories of Adolescent Thriving and Contribution Through the Words of Youth: Qualitative Findings from the 4-H Study of Positive Youth Development},
	volume = {43},
	issn = {0047-2891, 1573-6601},
	url = {http://link.springer.com/article/10.1007/s10964-014-0102-2},
	doi = {10.1007/s10964-014-0102-2},
	shorttitle = {Illuminating Trajectories of Adolescent Thriving and Contribution Through the Words of Youth},
	abstract = {Theory and research in adolescent development have emphasized that contributing to self, others, and community is important to the success of society and predictive of positive youth and later adult development. Despite this emphasis, there is a lack of qualitative and youth-centered research exploring whether adolescents themselves value contribution as part of their daily lives or future goals. Understandings of contribution are, thus, limited in their generalizability. To lessen this gap, we implemented qualitative analyses of open-ended responses from youth in the 4-H Study of Positive Youth Development. We addressed questions about what is meaningful to youth and about their future goals through descriptive and thematic analyses of responses from 56 youth (66 \% female) who participated in the 4-H Study in each of three grades (6, 9, and 12). Findings indicated that most youth in this study valued acts and/or ideologies of contribution at some point in their adolescence, and several were committed to facets of contribution across grades. The analyses also identified other aspects of these youth experiences (e.g., athletics, family relationships, and academic competencies) that were described as meaningful across adolescence. Findings are discussed in relationship to youth programming aimed at encouraging well-being and contribution in adolescence.},
	pages = {950--970},
	number = {6},
	journaltitle = {Journal of Youth and Adolescence},
	shortjournal = {J Youth Adolescence},
	author = {Hershberg, Rachel M. and {DeSouza}, Lisette M. and Warren, Amy E. A. and Lerner, Jacqueline V. and Lerner, Richard M.},
	urldate = {2015-11-19},
	date = {2014-02-21},
	langid = {english},
	keywords = {Adolescence, Child and School Psychology, Clinical Psychology, Health Psychology, History of Psychology, Law and Psychology, Psychology, general, positive youth development, Contribution, Qualitative, Descriptive analysis},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/RSNFNB3U/Hershberg et al. - 2014 - Illuminating Trajectories of Adolescent Thriving a.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4Q34PES4/s10964-014-0102-2.html:text/html}
}

@article{arbeit_profiles_2014,
	title = {Profiles of Problematic Behaviors Across Adolescence: Covariations with Indicators of Positive Youth Development},
	volume = {43},
	issn = {0047-2891, 1573-6601},
	url = {http://link.springer.com/article/10.1007/s10964-014-0092-0},
	doi = {10.1007/s10964-014-0092-0},
	shorttitle = {Profiles of Problematic Behaviors Across Adolescence},
	abstract = {Previous analyses of data from the 4-H Study of Positive Youth Development ({PYD}) have examined concurrent trajectories of positive development and risk/problem behaviors among adolescents, finding complex and not necessarily inverse relationships among them. In this article, we expand on prior research by employing a person-centered approach to modeling risk behaviors, assessing development from approximately 6th grade through 12th grade among 4,391 adolescents (59.9 \% female). Latent profiles involving the problematic behaviors of delinquency, depressive symptoms, substance use, sexual activity, disordered eating behaviors, and bullying were then assessed for concurrent relationships with the Five Cs of {PYD}: Competence, Confidence, Character, Caring, and Connection. We found six latent profiles, based primarily on mental health, aggression, and alcohol use, with significant differences in Confidence levels among many of the profiles, as well as some differences in the four other Cs. We discuss directions for future research and implications for application to youth policies and programs.},
	pages = {971--990},
	number = {6},
	journaltitle = {Journal of Youth and Adolescence},
	shortjournal = {J Youth Adolescence},
	author = {Arbeit, Miriam R. and Johnson, Sara K. and Champine, Robey B. and Greenman, Kathleen N. and Lerner, Jacqueline V. and Lerner, Richard M.},
	urldate = {2015-11-19},
	date = {2014-02-22},
	langid = {english},
	keywords = {Child and School Psychology, Clinical Psychology, Health Psychology, History of Psychology, Law and Psychology, Psychology, general, positive youth development, Risk behaviors, Profile analysis},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/UT3UXBWB/Arbeit et al. - 2014 - Profiles of Problematic Behaviors Across Adolescen.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/5GVQKCCP/s10964-014-0092-0.html:text/html}
