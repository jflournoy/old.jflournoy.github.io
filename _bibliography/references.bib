	date = {2014-01-30},
	langid = {english},
	keywords = {Academic achievement, Child and School Psychology, Clinical Psychology, Health Psychology, History of Psychology, Law and Psychology, Psychology, general, School engagement, Confirmatory factor analysis, Longitudinal structural equation modeling},
