@article{saxe_brain_2009,
	title = {Brain Regions for Perceiving and Reasoning About Other People in School-Aged Children},
	volume = {80},
	rights = {© 2009, Copyright the Author(s). Journal Compilation © 2009, Society for Research in Child Development, Inc.},
