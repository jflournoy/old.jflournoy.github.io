@article{okeefe_asymmetry_2011,
	title = {The Asymmetry of Predictive and Descriptive Capabilities in Quantitative Communication Research: Implications for Hypothesis Development and Testing},
	volume = {5},
	issn = {1931-2458},
	url = {http://dx.doi.org/10.1080/19312458.2011.568375},
	doi = {10.1080/19312458.2011.568375},
	shorttitle = {The Asymmetry of Predictive and Descriptive Capabilities in Quantitative Communication Research},
	abstract = {The bulk of hypotheses in quantitative communication research are directional (e.g., the correlation is positive, the treatment mean is larger than the control mean). For testing such hypotheses, null hypothesis significance testing ({NHST}) and the use of effect sizes and confidence intervals ({ES}+{CI}) are functionally equivalent. {ES}+{CI} provides more precise descriptions of research results (effect sizes, confidence intervals) than does {NHST}, but that descriptive capability exceeds researchers' current predictive capabilities. Developing more refined predictive capabilities will require making good use of the additional information provided by {ES}+{CI}—and careful thinking about how such refined hypotheses might be tested.},
	pages = {113--125},
	number = {2},
	journaltitle = {Communication Methods and Measures},
	author = {O'Keefe, Daniel J.},
	urldate = {2016-07-15},
	date = {2011-04-01},
	file = {Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/W496EF7G/19312458.2011.html:text/html}
}

@article{chen_is_2016,
	title = {Is the Statistic Value All We Should Care about in Neuroimaging?},
	rights = {© 2016, Published by Cold Spring Harbor Laboratory Press. This pre-print is available under a Creative Commons License (Attribution-{NonCommercial}-{NoDerivs} 4.0 International), {CC} {BY}-{NC}-{ND} 4.0, as described at http://creativecommons.org/licenses/by-nc-nd/4.0/},
	url = {http://biorxiv.org/content/early/2016/07/15/064212},
	doi = {10.1101/064212},
	abstract = {Here we address an important issue that has been embedded within the neuroimaging community for a long time: the absence of effect estimates in results reporting in the literature. The statistic value itself, as a dimensionless measure, does not provide information on the biophysical interpretation of a study, and it certainly does not represent the whole picture of a study. Unfortunately, in contrast to standard practice in most scientific fields, effect (or amplitude) estimates are usually not provided in most results reporting in the current neuroimaging publications and presentations. Possible reasons underlying this general trend include: 1) lack of general awareness, 2) software limitations, 3) inaccurate estimation of the {BOLD} response, and 4) poor modeling due to our relatively limited understanding of {FMRI} signal components. However, as we discuss here, such reporting damages the reliability and interpretability of the scientific findings themselves, and there is in fact no overwhelming reason for such a practice to persist. In order to promote meaningful interpretation, cross validation, reproducibility, meta and power analyses in neuroimaging, we strongly suggest that, as part of good scientific practice, effect estimates should be reported together with their corresponding statistic values. We provide several easily adaptable recommendations for facilitating this process.},
	pages = {064212},
	journaltitle = {{bioRxiv}},
	author = {Chen, Gang and Taylor, Paul A. and Cox, Robert W.},
	urldate = {2016-07-16},
	date = {2016-07-15},
	langid = {english},
	file = {Chen et al. - 2016 - Is the Statistic Value All We Should Care about in.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JR2PAFU7/Chen et al. - 2016 - Is the Statistic Value All We Should Care about in.pdf:application/pdf;Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/QER3N68G/Chen et al. - 2016 - Is the Statistic Value All We Should Care about in.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/GTKRHUU5/064212.html:text/html;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/RFPUWK4B/064212.html:text/html}
}

