	date = {2011-01-01},
	langid = {english},
	keywords = {child abuse and neglect, child mental health, child welfare, evidence-based practice, implementation research},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/Z3A5DKNQ/Landsverk et al. - 2011 - Bridging science and practice in child welfare and.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JDFV8U7H/80.html:text/html}
