	author = {Janowski, Vanessa and Camerer, Colin and Rangel, Antonio},
	urldate = {2014-04-29},
	date = {2013-02-01},
	langid = {english},
	pmid = {22349798},
	keywords = {Decision Making, Empathy, {IPL}, neuroeconomics, valuation, {vmPFC}},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/6ZB9SN3C/Janowski et al. - 2013 - Empathic choice involves vmPFC value signals that .pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/SN2X923R/201.html:text/html}
