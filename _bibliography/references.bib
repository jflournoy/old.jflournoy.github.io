	urldate = {2014-03-15},
	date = {2014-01-24},
	langid = {english},
	pmid = {24464847},
	keywords = {Brain, Empathy, {MRI}, Personality, fantasy, identification},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FWWX8K2P/Cheetham et al. - 2014 - Identifying with fictive characters structural br.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/IC34Q8SC/scan.nst179.html:text/html}
