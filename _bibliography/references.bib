	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FSQQXVXB/Nooner et al. - 2012 - The NKI-Rockland Sample A Model for Accelerating .pdf:application/pdf}
