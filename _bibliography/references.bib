@article{critchley_neural_2004,
	title = {Neural systems supporting interoceptive awareness},
	volume = {7},
	rights = {© 2004 Nature Publishing Group},
	issn = {1097-6256},
	url = {http://www.nature.com/neuro/journal/v7/n2/full/nn1176.html},
	doi = {10.1038/nn1176},
	abstract = {Influential theories of human emotion argue that subjective feeling states involve representation of bodily responses elicited by emotional events. Within this framework, individual differences in intensity of emotional experience reflect variation in sensitivity to internal bodily responses. We measured regional brain activity by functional magnetic resonance imaging ({fMRI}) during an interoceptive task wherein subjects judged the timing of their own heartbeats. We observed enhanced activity in insula, somatomotor and cingulate cortices. In right anterior insular/opercular cortex, neural activity predicted subjects' accuracy in the heartbeat detection task. Furthermore, local gray matter volume in the same region correlated with both interoceptive accuracy and subjective ratings of visceral awareness. Indices of negative emotional experience correlated with interoceptive accuracy across subjects. These findings indicate that right anterior insula supports a representation of visceral responses accessible to awareness, providing a substrate for subjective feeling states.},
	pages = {189--195},
	number = {2},
	journaltitle = {Nature Neuroscience},
	shortjournal = {Nat Neurosci},
	author = {Critchley, Hugo D. and Wiens, Stefan and Rotshtein, Pia and Öhman, Arne and Dolan, Raymond J.},
	urldate = {2013-03-19},
	date = {2004-02},
	langid = {english},
	keywords = {Behavior, Brain, Cerebellum, Hypothalamus, Learning, Memory, Thalamus, astrocyte, brain research, brain scientist, brainstem, cognitive science, computation, content, cortex, functional imaging, glia, journal, model, molecular biology, nature, nature neuroscience, nature publishing group, nervous system, neurodegeneration, neurodegenerative disease, neuron, neuroscience, neuroscientist, oligodendrocyte, physiology, psychophysics, retina, spinal cord, spinal cord injury, systems neuroscience},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/66XA7XTV/Critchley et al. - 2004 - Neural systems supporting interoceptive awareness.pdf:application/pdf;Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/G6NRW26A/Critchley et al. - 2004 - Neural systems supporting interoceptive awareness.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/H4V9E5IS/nn1176.html:text/html;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/V883TXBC/nn1176.html:text/html}
