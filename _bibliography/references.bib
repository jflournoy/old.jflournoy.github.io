	author = {Neubert, Franz-Xaver and Mars, Rogier B. and Sallet, Jérôme and Rushworth, Matthew F. S.},
	urldate = {2015-06-19},
	date = {2015-05-19},
	langid = {english},
	pmid = {25947150},
	keywords = {Anterior cingulate cortex, Decision Making, Orbitofrontal cortex, comparative anatomy, resting state functional connectivity},
	file = {Neubert et al. - 2015 - Connectivity reveals relationship of brain areas f.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/X7PMVHPK/Neubert et al. - 2015 - Connectivity reveals relationship of brain areas f.pdf:application/pdf;Neubert et al. - 2015 - Connectivity reveals relationship of brain areas f.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/KG4DVKUD/Neubert et al. - 2015 - Connectivity reveals relationship of brain areas f.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WZ87NK9T/E2695.abstract.html:text/html}
