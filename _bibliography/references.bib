@article{gregorich_self-report_2006,
	title = {Do Self-Report Instruments Allow Meaningful Comparisons Across Diverse Population Groups? Testing Measurement Invariance Using the Confirmatory Factor Analysis Framework},
	volume = {44},
	issn = {0025-7079},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1808350/},
	doi = {10.1097/01.mlr.0000245454.12228.8f},
	shorttitle = {Do Self-Report Instruments Allow Meaningful Comparisons Across Diverse Population Groups?},
	abstract = {Comparative public health research makes wide use of self-report instruments. For example, research identifying and explaining health disparities across demographic strata may seek to understand the health effects of patient attitudes or private behaviors. Such personal attributes are difficult or impossible to observe directly and are often best measured by self-reports. Defensible use of self-reports in quantitative comparative research requires not only that the measured constructs have the same meaning across groups, but also that group comparisons of sample estimates (eg, means and variances) reflect true group differences and are not contaminated by group-specific attributes that are unrelated to the construct of interest. Evidence for these desirable properties of measurement instruments can be established within the confirmatory factor analysis ({CFA}) framework; a nested hierarchy of hypotheses is tested that addresses the cross-group invariance of the instrument’s psychometric properties. By name, these hypotheses include configural, metric (or pattern), strong (or scalar), and strict factorial invariance. The {CFA} model and each of these hypotheses are described in nontechnical language. A worked example and technical appendices are included.},
	pages = {S78--S94},
	number = {11},
	journaltitle = {Medical care},
	shortjournal = {Med Care},
	author = {Gregorich, Steven E.},
	urldate = {2013-05-17},
	date = {2006-11},
	pmid = {17060839},
	pmcid = {PMC1808350},
	file = {Gregorich - 2006 - Do Self-Report Instruments Allow Meaningful Compar.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FIGTQSM5/Gregorich - 2006 - Do Self-Report Instruments Allow Meaningful Compar.pdf:application/pdf;Gregorich - 2006 - Do Self-Report Instruments Allow Meaningful Compar.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/GVPFEFVN/Gregorich - 2006 - Do Self-Report Instruments Allow Meaningful Compar.pdf:application/pdf}
}

@article{shariff_god_2007,
	title = {God Is Watching You Priming God Concepts Increases Prosocial Behavior in an Anonymous Economic Game},
	volume = {18},
	issn = {0956-7976, 1467-9280},
	url = {http://pss.sagepub.com/content/18/9/803},
	doi = {10.1111/j.1467-9280.2007.01983.x},
	abstract = {We present two studies aimed at resolving experimentally whether religion increases prosocial behavior in the anonymous dictator game. Subjects allocated more money to anonymous strangers when God concepts were implicitly activated than when neutral or no concepts were activated. This effect was at least as large as that obtained when concepts associated with secular moral institutions were primed. A trait measure of self-reported religiosity did not seem to be associated with prosocial behavior. We discuss different possible mechanisms that may underlie this effect, focusing on the hypotheses that the religious prime had an ideomotor effect on generosity or that it activated a felt presence of supernatural watchers. We then discuss implications for theories positing religion as a facilitator of the emergence of early large-scale societies of cooperators.},
