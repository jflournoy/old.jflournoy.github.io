	urldate = {2016-11-19},
	date = {2016-01-01},
	keywords = {data sharing, Neuroinformatics, Data Exchange, {DX}, {COINS}},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/K8GUTKU8/Landis et al. - 2016 - COINS Data Exchange An open platform for compilin.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/CDN9XQ8R/S1053811915004310.html:text/html}
}

