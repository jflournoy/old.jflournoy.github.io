	file = {Pereira et al. - 2009 - Machine learning classifiers and fMRI A tutorial .pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/722742TV/Pereira et al. - 2009 - Machine learning classifiers and fMRI A tutorial .pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/X43HZ8MZ/S1053811908012263.html:text/html}
}

