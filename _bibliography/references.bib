}

@article{harrigan_vanderbilt_2016,
	title = {Vanderbilt University Institute of Imaging Science Center for Computational Imaging {XNAT}: A multimodal data archive and processing environment},
	volume = {124, Part B},
	issn = {1053-8119},
	url = {http://www.sciencedirect.com/science/article/pii/S1053811915004012},
	doi = {10.1016/j.neuroimage.2015.05.021},
