	keywords = {Magnetic Resonance Imaging, Data repository, Ultra-high field, Quantitative imaging, High-resolution imaging, Relaxometry},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/KZIU6EHP/Tardif et al. - 2016 - Open Science CBS Neuroimaging Repository Sharing .pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/DTW8Q2Q5/S1053811915007612.html:text/html}
