	keywords = {{MRI}, {EEG}, Database, {MEG}, Open-access, Electrophysiology},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/N6KZPUF2/Niso et al. - 2016 - OMEGA The Open MEG Archive.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/MXMQ4GX9/S1053811915003183.html:text/html}
