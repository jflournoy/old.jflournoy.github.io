@article{zaki_need_2009,
	title = {The Need for a Cognitive Neuroscience of Naturalistic Social Cognition},
	volume = {1167},
	rights = {© 2009 New York Academy of Sciences},
	issn = {1749-6632},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1749-6632.2009.04601.x/abstract},
	doi = {10.1111/j.1749-6632.2009.04601.x},
	abstract = {Understanding the minds of others is one of the great challenges humans face. Accordingly, much work in cognitive neuroscience has explored the brain systems engaged when perceivers share and make inferences about the internal states of social targets. These studies, however, typically use divergent and highly simplified stimuli and methods and as a consequence have produced largely non-overlapping sets of results and artificially constrained theories about the processes involved in perceivers’ abilities to understand targets. Here we suggest that these difficulties may stem from two main sources: the lack of meaningful behavioral data about the brain bases of perceivers’ accuracy in inferring target states and qualitative differences between the social stimuli used in neuroimaging paradigms and the social information perceivers encounter in the real world. We advocate more focus on studies of naturalistic social cognition, which could overcome these limitations and complement current approaches, and discuss work in our laboratory that has demonstrated the feasibility and utility of such a focus. Finally, we discuss the relevance of naturalistic social cognition to diagnosing and treating autism spectrum disorder. Overall, using naturalistic paradigms in neuroimaging will be critical to modeling the way the brain actually understands other minds.},
	pages = {16--30},
	number = {1},
	journaltitle = {Annals of the New York Academy of Sciences},
	author = {Zaki, Jamil and Ochsner, Kevin},
	urldate = {2013-03-20},
	date = {2009},
	langid = {english},
	keywords = {Empathy, Social cognition, medial prefrontal cortex, mirror neurons},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/5TRURDJD/Zaki and Ochsner - 2009 - The Need for a Cognitive Neuroscience of Naturalis.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/EM6Q6T25/full.html:text/html}
}

@article{decety_power_2006,
	title = {The power of simulation: Imagining one's own and other's behavior},
	volume = {1079},
	issn = {0006-8993},
	url = {http://www.sciencedirect.com/science/article/pii/S0006899306000102},
	doi = {10.1016/j.brainres.2005.12.115},
	series = {Multiple Perspectives on the Psychological and Neural Bases of Understanding Other People's Behavior},
	shorttitle = {The power of simulation},
	abstract = {A large number of cognitive neuroscience studies point to the similarities in the neural circuits activated during the generation, imagination, as well as observation of one's own and other's behavior. Such findings support the shared representations account of social cognition, which is suggested to provide the basic mechanism for social interaction. Mental simulation may also be a representational tool to understand the self and others. However, successfully navigating these shared representations – both within oneself and between individuals – constitutes an essential functional property of any autonomous agent. It will be argued that self-awareness and agency, mediated by the temporoparietal ({TPJ}) area and the prefrontal cortex, are critical aspects of the social mind. Thus, differences as well as similarities between self and other representations at the neural level may be related to the degrees of self-awareness and agency. Overall, these data support the view that social cognition draws on both domain-general mechanisms and domain-specific embodied representations.},
	pages = {4--14},
	number = {1},
	journaltitle = {Brain Research},
	shortjournal = {Brain Research},
	author = {Decety, Jean and Grèzes, Julie},
	urldate = {2014-04-30},
	date = {2006-03-24},
	keywords = {Empathy, Functional Neuroimaging, Intersubjectivity, Self-agency, Shared representation, Social neuroscience},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/GV3NI2VD/Decety and Grèzes - 2006 - The power of simulation Imagining one's own and o.pdf:application/pdf;ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JDU78KTE/Decety and Grèzes - 2006 - The power of simulation Imagining one's own and o.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/J9PES2I7/S0006899306000102.html:text/html;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/U2238KEW/S0006899306000102.html:text/html}
