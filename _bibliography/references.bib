@article{salmela-aro_young_2012,
	title = {Young Adults' Developmental Task-Related Goals Modify the Association between Self-Focused Goals and Depressive Symptoms},
	volume = {4},
	rights = {© 2011 The Authors. Applied Psychology: Health and Well-Being © 2011 The International Association of Applied Psychology},
