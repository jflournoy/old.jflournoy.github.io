@article{reimold_central_2010,
	title = {Central serotonin transporter levels are associated with stress hormone response and anxiety},
	issn = {1432-2072},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/20585760},
	doi = {10.1007/s00213-010-1903-y},
	abstract = {{RATIONALE}: Negative mood states are characterized by both stress hormone dysregulation and serotonergic dysfunction, reflected by altered thalamic serotonin transporter (5-{HTT}) levels. However, so far, no study examined the individual association between cortisol response and cerebral in vivo 5-{HTT} levels in patients suffering from negative mood states. {OBJECTIVE}: The objective of this cross-sectional study was to assess the interrelation of cortisol response, thalamic 5-{HTT} levels, and anxiety in healthy subjects and two previously published samples of patients with unipolar major depression ({UMD}) and obsessive-compulsive disorder ({OCD}), controlling for age, gender, 5-{HTT} genotype, smoking, and seasonality. {METHODS}: Regional 5-{HTT} levels and cortisol response to dexamethasone-corticotropin (Dex-{CRH}) challenge were assessed in consecutive samples of medication-free patients suffering from {UMD} (N = 10) and {OCD} (N = 10), and 20 healthy volunteers. The intervention used was combined Dex-{CRH} test and [(11)C]{DASB} positron emission tomography. The main outcome measures were: 5-{HTT} binding potential ({BP}({ND})) in a predefined thalamic {ROI}, cortisol response defined as the maximum cortisol increase in the combined Dex-{CRH}-test, and state of anxiety from the state-trait-anxiety inventory. {RESULTS}: Reduced thalamic 5-{HTT} {BP}({ND}) was associated with increased cortisol response (r = -0.35, p {\textless} 0.05; in patients: r = -0.53, p {\textless} 0.01) and with increased state anxiety (r = -0.46, p {\textless} 0.01), surviving correction for age, gender, 5-{HTT} genotype, smoking, and seasonality (p {\textless} 0.05). The 5-{HTT} genotype, on the contrary, was not significantly associated with cortisol response (p = 0.19) or negative mood (p = 0.23). {CONCLUSION}: The association between stress hormone response, thalamic 5-{HTT} levels, and anxiety in patients suffering from negative mood states suggests an interaction between two major mechanisms implicated in negative mood states in humans.},
	journaltitle = {Psychopharmacology},
	shortjournal = {Psychopharmacology (Berl)},
	author = {Reimold, Matthias and Knobel, Astrid and Rapp, Michael A and Batra, Anil and Wiedemann, Klaus and Ströhle, Andreas and Zimmer, Anke and Schönknecht, Peter and Smolka, Michael N and Weinberger, Daniel R and Goldman, David and Machulla, Hans-Jürgen and Bares, Roland and Heinz, Andreas},
	urldate = {2010-08-11},
	date = {2010-06-29},
	pmid = {20585760}
}

@article{rapp_where_2012,
	title = {Where in the brain is nonliteral language? A coordinate-based meta-analysis of functional magnetic resonance imaging studies},
	volume = {63},
	issn = {1053-8119},
	url = {http://www.sciencedirect.com/science/article/pii/S1053811912006362},
	doi = {10.1016/j.neuroimage.2012.06.022},
	shorttitle = {Where in the brain is nonliteral language?},
	abstract = {An increasing number of studies have investigated non-literal language, including metaphors, idioms, metonymy, or irony, with functional magnetic resonance imaging ({fMRI}). However, key questions regarding its neuroanatomy remain controversial. In this work, we used coordinate-based activation-likelihood estimations to merge available {fMRI} data on non-literal language.
A literature search identified 38 {fMRI} studies on non-literal language (24 metaphor studies, 14 non-salient stimuli studies, 7 idiom studies, 8 irony studies, and 1 metonymy study). Twenty-eight studies with direct comparisons of non-literal and literal studies were included in the main meta-analysis. Sub-analyses for metaphors, idioms, irony, salient metaphors, and non-salient metaphors as well as studies on sentence level were conducted.
Studies reported 409 activation foci, of which 129 (32\%) were in the right hemisphere. These meta-analyses indicate that a predominantly left lateralised network, including the left and right inferior frontal gyrus; the left, middle, and superior temporal gyrus; and medial prefrontal, superior frontal, cerebellar, parahippocampal, precentral, and inferior parietal regions, is important for non-literal expressions.},
	pages = {600--610},
	number = {1},
	journaltitle = {{NeuroImage}},
	shortjournal = {{NeuroImage}},
	author = {Rapp, Alexander M. and Mutschler, Dorothee E. and Erb, Michael},
	urldate = {2014-05-09},
	date = {2012-10-15},
	keywords = {Figurative, Inferior frontal gyrus, Lateralisation, Metaphor, Middle temporal gyrus, Motor, Pragmatic, Right hemisphere, Semantic, Sentence},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4T9T4V3T/Rapp et al. - 2012 - Where in the brain is nonliteral language A coord.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4EGUFFJ8/S1053811912006362.html:text/html}
