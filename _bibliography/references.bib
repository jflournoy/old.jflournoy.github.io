@article{watson_dex/crh_2006,
	title = {The dex/{CRH} test—Is it better than the {DST}?},
	volume = {31},
	issn = {0306-4530},
	url = {http://www.sciencedirect.com/science/article/pii/S0306453006000503},
	doi = {10.1016/j.psyneuen.2006.03.001},
	abstract = {Summary
The dexamethasone suppression test ({DST}), frequently abnormal in mood disorder patients, is considered to measure glucocorticoid receptor-mediated negative feedback. We examined the hypothesis that the, apparently more sensitive, dexamethasone/corticotrophin-releasing-hormone (dex/{CRH}) test unveils subtle hypothalamicâ€“pituitaryâ€“adrenal axis disturbance not detected by the {DST} in 82 patients with mood disorders and 28 controls. There was a close correlation between the cortisol responses on the two tests (rs=0.73, p\&lt;0.0005). However, {ROC} analysis revealed that the dex/{CRH} test had better diagnostic performance than the {DST} (p=0.031). The sensitivity of delta cortisol (from the dex/{CRH}) was 61.9\% and the specificity 71.4\%. The sensitivity of 1500\&\#xa0;h cortisol (the {DST}) was 66.6\% and the specificity was 47.6\%. This suggests that the two tests measure common pathology but that the dex/{CRH} test has better diagnostic utility.},
	pages = {889--894},
	number = {7},
	journaltitle = {Psychoneuroendocrinology},
	author = {Watson, Stuart and Gallagher, Peter and Smith, Margaret S. and Ferrier, I. Nicol and Young, Allan H.},
	urldate = {2011-10-01},
	date = {2006-08},
	keywords = {Adrenal cortex hormones, Corticotrophin releasing hormone, Cortisol, {DST}, Dexamethasone, depression, dex/{CRH} test},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/I2EXNZ3V/Watson et al. - 2006 - The dexCRH testâ€”Is it better than the DST.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/A8QV8SZ7/S0306453006000503.html:text/html}
}

@article{tordjman_role_2001,
	title = {Role of the serotonin transporter gene in the behavioral expression of autism.},
	volume = {6},
	issn = {13594184},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=a2h&AN=8992466&site=ehost-live},
	doi = {Article},
	abstract = {The promoter polymorphism of the serotonin transporter gene ({HTT}, locus {SLC}6A4) is of special interest in autism given the well-replicated platelet hyperserotonemia of autism, treatment effects of serotonin reuptake inhibitors, and the role of serotonin in limbic functioning and neurodevelopment. Parent-offspring transmission of the long (/) and short (s) alleles of the deletion/insertion polymorphism in the {HTT} promoter region was examined in families of 71 children with autism using the transmission test for linkage disequilibrium ({TDT}). Transmission of {HTT} promoter alleles did not differ between probands with autism and their unaffected siblings. However, allelic transmission in probands was dependent upon severity of impairments in the social and communication domains, with greater s allele transmission in severely impaired individuals and greater I transmission in mild/moderately impaired individuals. This relationship between {HTT} promoter alleles and severity of autistic impairment was also seen when ratings of social and communication behaviors were compared across genotypes. The data indicate that {HTT} promoter alleles by themselves do not convey risk for autism, but, rather, modify the severity of autistic behaviors in the social and communication domains. The results require replication and, given the size of the groups and subgroups examined, must be considered still preliminary. The results suggest that future research on the genetics of autism should carefully assess each of the major behavioral domains and seriously consider the possible role of modifying loci. [{ABSTRACT} {FROM} {AUTHOR}]},
	pages = {434},
	number = {4},
	journaltitle = {Molecular Psychiatry},
	author = {Tordjman, S. and Gutknecht, L. and Carlier, M. and Spitz, E. and Antoine, C. and Slama, F. and Carsalade, V. and Cohen, D.J. and Ferrari, P. and Roubertoux, P.L. and Anderson, G.M.},
	urldate = {2009-11-07},
	date = {2001-07},
	keywords = {Autism, {GENETIC} aspects, {GENETIC} polymorphisms, {SEROTONIN}},
	file = {Tordjman et al. - 2001 - Role of the serotonin transporter gene in the beha.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/Z7FQXBRA/Tordjman et al. - 2001 - Role of the serotonin transporter gene in the beha.pdf:application/pdf}
}

@article{saperstein_racial_2012,
	title = {Racial Fluidity and Inequality in the United States},
	volume = {118},
	rights = {Copyright © 2012 The University of Chicago Press},
	issn = {0002-9602},
	url = {http://www.jstor.org/stable/10.1086/667722},
	doi = {10.1086/667722},
	abstract = {The authors link the literature on racial fluidity and inequality in the United States and offer new evidence of the reciprocal relationship between the two processes. Using two decades of longitudinal data from a national survey, they demonstrate that not only does an individual’s race change over time, it changes in response to myriad changes in social position, and the patterns are similar for both self-identification and classification by others. These findings suggest that, in the contemporary United States, microlevel racial fluidity serves to reinforce existing disparities by redefining successful or high-status people as white (or not black) and unsuccessful or low-status people as black (or not white). Thus, racial differences are both an input and an output in stratification processes; this relationship has implications for theorizing and measuring race in research, as well as for crafting policies that attempt to address racialized inequality.},
	pages = {676--727},
	number = {3},
	journaltitle = {American Journal of Sociology},
	shortjournal = {American Journal of Sociology},
	author = {Saperstein, Aliya and Penner, Andrew M.},
	urldate = {2013-11-01},
	date = {2012-11-01},
	note = {{ArticleType}: research-article / Full publication date: November 2012 / Copyright © 2012 The University of Chicago Press},
	file = {JSTOR Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/7GHTE6FW/Saperstein and Penner - 2012 - Racial Fluidity and Inequality in the United State.pdf:application/pdf}
}

