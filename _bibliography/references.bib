@article{raznahan_serotonin_2009,
	title = {Serotonin transporter genotype and neuroanatomy in autism spectrum disorders},
	volume = {19},
	issn = {1473-5873},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/19369912},
	doi = {10.1097/YPG.0b013e32832a505a},
	abstract = {There is increasing evidence that people with autism spectrum disorders ({ASDs}) have abnormalities in the serotonergic system. For example, a functional polymorphism of the serotonin transporter gene promoter region (5HTTLPR long/short polymorphism) has been reported to confer risk for {ASDs}, and to affect cortical grey matter volume in young children. However, the persistence of this association later in development is unknown. Hence, we investigated whether variation in the 5HTTLPR long/short polymorphism modulates brain anatomy in older people with {ASD}. We related 5HTTLPR long/short polymorphism in 43 adolescents and adults with {ASD} to brain anatomy using structural magnetic resonance imaging and voxel-based morphometry. There were no significant associations between brain anatomy and genotype. When considered alongside evidence of a relationship between 5HTTLPR genotype and brain volume amongst children with autism, our findings raise the possibility that the relationship between 5HTTLPR polymorphism and brain anatomy in {ASDs} anatomy may differ as a function of age and/or {ASD} subdiagnosis.},
	pages = {147--150},
	number = {3},
	journaltitle = {Psychiatric Genetics},
	shortjournal = {Psychiatr. Genet},
	author = {Raznahan, Armin and Pugliese, Luca and Barker, Gareth J and Daly, Eileen and Powell, John and Bolton, Patrick F and Murphy, Declan G M},
	urldate = {2010-02-06},
	date = {2009-06},
	pmid = {19369912},
	keywords = {Adolescent, Adult, Autistic Disorder, Base Sequence, Brain, Child, {DNA} Primers, Genotype, Humans, Male, Middle Aged, Polymorphism, Genetic, Promoter Regions, Genetic, Serotonin Plasma Membrane Transport Proteins, Young Adult}
}

