@article{kraemer_mediators_2002,
	title = {Mediators and Moderators of Treatment Effects in Randomized Clinical Trials},
	volume = {59},
	url = {http://archpsyc.ama-assn.org/cgi/content/abstract/59/10/877},
	doi = {<p>10.1001/archpsyc.59.10.877</p>},
	abstract = {Randomized clinical trials ({RCTs}) not only are the gold standard for evaluating the efficacy and effectiveness of psychiatric treatments but also can be valuable in revealing moderators and mediators of therapeutic change. Conceptually, moderators identify on whom and under what circumstances treatments have different effects. Mediators identify why and how treatments have effects. We describe an analytic framework to identify and distinguish between moderators and mediators in {RCTs} when outcomes are measured dimensionally. Rapid progress in identifying the most effective treatments and understanding on whom treatments work and do not work and why treatments work or do not work depends on efforts to identify moderators and mediators of treatment outcome. We recommend that {RCTs} routinely include and report such analyses.},
	pages = {877--883},
	number = {10},
	journaltitle = {Arch Gen Psychiatry},
	author = {Kraemer, Helena Chmura and Wilson, G. Terence and Fairburn, Christopher G. and Agras, W. Stewart},
	urldate = {2011-03-28},
	date = {2002-10-01},
	file = {HighWire Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/4PSDHZIA/Kraemer et al. - 2002 - Mediators and Moderators of Treatment Effects in R.pdf:application/pdf;HighWire Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/STZDTDCR/877.html:text/html}
}

@article{friedman_autonomic_1998,
	title = {Autonomic balance revisited: Panic anxiety and heart rate variability},
	volume = {44},
	issn = {0022-3999},
	url = {http://www.sciencedirect.com/science/article/pii/S002239999700202X},
	doi = {10.1016/S0022-3999(97)00202-X},
	shorttitle = {Panic Disorder in General Medicine},
	abstract = {The analysis of heart rate variability ({HRV}) is becoming widely used in clinical research to provide a window into autonomic control of {HR}. This technique has been valuable in elucidating the autonomic underpinnings of panic disorder ({PD}), a condition that is marked by reports of heart palpitations. A body of research has emerged that implicates a relative reduction in {HRV} and cardiac vagal tone in {PD}, as indicated by various {HRV} measures. These data are consistent with the cardiac symptoms of panic attacks, as well as with developmental evidence that links high vagal tone with enhanced attention, effective emotion regulation, and organismic responsivity. Implications of these findings for nosology and pathophysiology are discussed. Reports of reduced {HRV} in {PD} contrast with portrayals of excess autonomic lability in anxiety. This contradiction is addressed in the context of traditional homeostatic models versus a systems perspective that views physiologic variability as essential to overall stability.},
	pages = {133--151},
	number = {1},
	journaltitle = {Journal of Psychosomatic Research},
	shortjournal = {Journal of Psychosomatic Research},
	author = {Friedman, Bruce H. and Thayer, Julian F.},
	urldate = {2013-06-12},
	date = {1998-01},
	keywords = {Autonomic nervous system, Heart rate variability, Panic Disorder},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/B4USNGXZ/Friedman and Thayer - 1998 - Autonomic balance revisited Panic anxiety and hea.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/258S7RW6/S002239999700202X.html:text/html}
}

