	file = {Neel_et_al-2015-Individual_Differences_in_Fundamental_Social_Motives.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/TK4RIT3Z/Neel_et_al-2015-Individual_Differences_in_Fundamental_Social_Motives.pdf:application/pdf;Neel_et_al-2015-Individual_Differences_in_Fundamental_Social_Motives.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/KEKD6J7X/Neel_et_al-2015-Individual_Differences_in_Fundamental_Social_Motives.pdf:application/pdf}
