@article{meehl_whats_2004,
	title = {What's in a Taxon?},
	volume = {113},
	rights = {(c) 2012 {APA}, all rights reserved},
	issn = {1939-1846(Electronic);0021-843X(Print)},
	doi = {10.1037/0021-843X.113.1.39},
	abstract = {Whether taxometrics yields inferential knowledge to something latent is partly but not wholly a semantic question. Although the single variables are manifest indicator scores of individuals, the statistics computed from them via postulates of the formalism are not mere data summaries and will be incorrect or meaningless if the structural conjectures are false. The unidirectional derivability from postulates to data relations supplies the taxonic inferences' surplus meaning that constitutes conventional psychometric meaning of latency (e.g., latent class analysis). Surplus meaning beyond the purely mathematical may be provided by interpretive text attributing unobserved attributes or causal origin to taxon members.},
	pages = {39--43},
	number = {1},
	journaltitle = {Journal of Abnormal Psychology},
	author = {Meehl, Paul E.},
	date = {2004},
	keywords = {*Statistical Analysis, Psychometrics, *Taxonomies, *Inference, *Meaning, *Psychodiagnostic Typologies, Item Response Theory, \_tablet},
	file = {Meehl-2004-What's_in_a_Taxon.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/WQKGQ2M2/Meehl-2004-What's_in_a_Taxon.pdf:application/pdf;Meehl-2004-What's_in_a_Taxon.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/C2S9SQ2U/Meehl-2004-What's_in_a_Taxon.pdf:application/pdf}
}

@article{meehl_clarifications_1999,
	title = {Clarifications about taxometric method},
	volume = {8},
	issn = {0962-1849},
	url = {http://www.sciencedirect.com/science/article/pii/S0962184905800757},
	doi = {10.1016/S0962-1849(05)80075-7},
	abstract = {Taxometrics is a statistical procedure for determining whether relationships among observables reflect the existence of a latent taxon (type, species, category, disease entity). A formal-numerical definition is needed because intuitive, commonsense notions of “carving nature at its joints” or “identifying natural kinds” cannot resolve disagreements as to taxonic reality for hard cases. Specific etiology (e.g., major gene, germ, traumatic event) is often unknown and is not appropriate in nonmedical domains. Lacking an infallible criterion, the taxonic inference relies on the internal configural relations among the conjectural fallible indicators. An essential feature is multiple consistency tests that will not be satisfied if the latent structure is not taxonic or the parameters are badly estimated. Common misconceptions are that the taxon must be “sharply” distinguished, quantitative indicators must be bimodal, the causal origin must be biological, emergence of a large dimensional factor refutes taxonicity, and adopting a taxon is a mere matter of convention or preference.},
	pages = {165--174},
	number = {3},
	journaltitle = {Applied and Preventive Psychology},
	shortjournal = {Applied and Preventive Psychology},
	author = {Meehl, Paul E.},
	urldate = {2016-06-17},
	date = {1999-06-01},
	keywords = {psychopathology, Diagnostic entities, Latent class, {MAMBAC}, {MAXCOV}, Psychometric method, Taxometrics, Taxonomy, {TAXSCAN}, Types},
	file = {Meehl - 1999 - Clarifications about taxometric method.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/8CRIWRSR/Meehl - 1999 - Clarifications about taxometric method.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/MFVKN5X3/S0962184905800757.html:text/html}
