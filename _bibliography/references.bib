	abstract = {Some commentators wholeheartedly disagreed with the central tenet of the network perspective on personality, namely that traits are the result of mutual interactions between thoughts, feelings and behaviours. In this rejoinder, we primarily focus on these commentaries by (i) clarifying the main differences between the latent versus the network view on traits; (ii) discussing some of the arguments in favour of the latent trait views that were put forward by these commentators; and by (iii) comparing the capacity of both views to explain thoughts, feelings and behaviours. Some commentators were by and large positive about the network perspective, and we discuss their excellent suggestions for defining components and linking these to genes and other biological mechanisms. We conclude that no doors should be closed in the study of personality and that, as such, alternative theories such as the network perspective should be welcomed, formalised and tested. Copyright © 2012 John Wiley \& Sons, Ltd.},
	pages = {451--459},
	number = {4},
	journaltitle = {European Journal of Personality},
	shortjournal = {Eur. J. Pers.},
	author = {Cramer, Angélique O. J. and van der Sluis, Sophie and Noordhof, Arjen and Wichers, Marieke and Geschwind, Nicole and Aggen, Steven H. and Kendler, Kenneth S. and Borsboom, Denny},
	urldate = {2015-05-20},
	date = {2012-07-01},
	langid = {english},
	keywords = {personality traits, latent trait perspective, network models},
	file = {Cramer et al. - 2012 - Measurable Like Temperature or Mereological Like F.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/9GJVK7D3/Cramer et al. - 2012 - Measurable Like Temperature or Mereological Like F.pdf:application/pdf;Cramer et al. - 2012 - Measurable Like Temperature or Mereological Like F.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/P5F8EUX7/Cramer et al. - 2012 - Measurable Like Temperature or Mereological Like F.pdf:application/pdf;Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/HTABHAWD/Cramer et al. - 2012 - Measurable Like Temperature or Mereological Like F.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/NJM4DMJ5/abstract\;jsessionid=59E8BF4202D2BB8C0E1E93094CFAA72A.html:text/html;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/5RXT9IVC/abstract.html:text/html}
}

@article{white_betweenness_1994,
	title = {Betweenness centrality measures for directed graphs},
	volume = {16},
	issn = {0378-8733},
	url = {http://www.sciencedirect.com/science/article/pii/0378873394900159},
	doi = {10.1016/0378-8733(94)90015-9},
	abstract = {This paper generalizes Freeman's geodesic centrality measures for betweenness on undirected graphs to the more general directed case. Four steps are taken. The point centrality measure is first generalized for directed graphs. Second, a unique maximally centralized graph is defined for directed graphs, holding constant the numbers of points with reciprocatable (incoming and outgoing) versus only unreciprocatable (outgoing only or incoming only) arcs, and focusing the measure on the maximally central arrangement of arcs within these constraints. Alternatively, one may simply normalize on the number of arcs. This enables the third step of defining the relative betweenness centralities of a point, independent of the number of points. This normalization step for directed centrality measures removes Gould's objection that centrality measures for directed graphs are not interpretable because they lack a standard for maximality. The relative directed centrality converges with Freeman's betweenness measure in the case of undirected graphs with no isolates. The fourth step is to define the measures of this concept of graph centralization in terms of the dominance of the most central point.},
	pages = {335--346},
	number = {4},
	journaltitle = {Social Networks},
	shortjournal = {Social Networks},
	author = {White, Douglas R. and Borgatti, Stephen P.},
	urldate = {2015-05-21},
	date = {1994-10},
	file = {ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/EPDIU2WZ/0378873394900159.html:text/html;White and Borgatti - 1994 - Betweenness centrality measures for directed graph.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/PREJ9STD/White and Borgatti - 1994 - Betweenness centrality measures for directed graph.pdf:application/pdf}
}

