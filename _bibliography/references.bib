@article{kloepfer_memory_2009,
	title = {Memory before and after sleep in patients with moderate obstructive sleep apnea},
	volume = {5},
	issn = {1550-9389},
	url = {http://www.ncbi.nlm.nih.gov.laneproxy.stanford.edu/pubmed/20465021},
	abstract = {{OBJECTIVE}: The aim of this study was to investigate the effects of obstructive sleep apnea ({OSA}) on procedural and declarative memory encoding in the evening prior to sleep, on memory consolidation during subsequent sleep, and on retrieval in the morning after sleep. {METHODS}: Memory performance (procedural mirror-tracing task, declarative visual and verbal memory task) and general neuropsychological performance were assessed before and after one night of polysomnographic monitoring in 15 patients with moderate {OSA} and 20 age-, sex-, and {IQ}-matched healthy subjects. {RESULTS}: Encoding levels prior to sleep were similar across groups for all tasks. Conventional analyses of averaged mirror tracing performance suggested a significantly reduced overnight improvement in {OSA} patients. Single trial analyses, however, revealed that this effect was due to significantly flattened learning curves in the evening and morning session in {OSA} patients. {OSA} patients showed a significantly lower verbal retention rate and a non-significantly reduced visual retention rate after sleep compared to healthy subjects. Polysomnography revealed a significantly reduced {REM} density, increased frequency of micro-arousals, elevated apnea-hypopnea index, and subjectively disturbed sleep quality in {OSA} patients compared to healthy subjects. {CONCLUSIONS}: The results suggest that moderate {OSA} is associated with a significant impairment of procedural and verbal declarative memory. Future work is needed to further determine the contribution of structural or functional alterations in brain circuits relevant for memory, and to test whether {OSA} treatment improves or normalizes the observed deficits in learning.},
	pages = {540--548},
	number = {6},
	journaltitle = {Journal of Clinical Sleep Medicine: {JCSM}: Official Publication of the American Academy of Sleep Medicine},
	shortjournal = {J Clin Sleep Med},
	author = {Kloepfer, Corinna and Riemann, Dieter and Nofzinger, Eric A and Feige, Bernd and Unterrainer, Josef and O'Hara, Ruth and Sorichter, Stephan and Nissen, Christoph},
	urldate = {2010-05-25},
	date = {2009-12-15},
	pmid = {20465021},
	keywords = {{OSA}, cognition, memory, sleep disturbance, sleep quality}
}

@article{thayer_model_2000,
	title = {A model of neurovisceral integration in emotion regulation and dysregulation},
	volume = {61},
	issn = {0165-0327},
	url = {http://www.sciencedirect.com/science/article/pii/S0165032700003384},
	doi = {10.1016/S0165-0327(00)00338-4},
	shorttitle = {Arousal in Anxiety},
	abstract = {In the present paper we present the outlines of a model that integrates autonomic, attentional, and affective systems into a functional and structural network that may help to guide us in our understanding of emotion regulation and dysregulation. We will emphasize the relationship between attentional regulation and affective processes and propose a group of underlying physiological systems that serve to integrate these functions in the service of self-regulation and adaptability of the organism. We will attempt to place this network in the context of dynamical systems models which involve feedback and feedforward circuits with special attention to negative feedback mechanisms, inhibitory processes, and their role in response selection. From a systems perspective, inhibitory processes can be viewed as negative feedback circuits that allow for the interruption of ongoing behavior and the re-deployment of resources to other tasks. When these negative feedback mechanisms are compromised, positive feedback loops may develop as a result (of dis-inhibition). From this perspective, the relative sympathetic activation seen in anxiety disorders may represent dis-inhibition due to faulty inhibitory mechanisms.},
	pages = {201--216},
	number = {3},
	journaltitle = {Journal of Affective Disorders},
	shortjournal = {Journal of Affective Disorders},
	author = {Thayer, Julian F and Lane, Richard D},
	urldate = {2013-06-12},
	date = {2000-12-02},
	keywords = {Emotion regulation and dysregulation, Neurovisceral integration},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/S5U9QA6C/Thayer and Lane - 2000 - A model of neurovisceral integration in emotion re.pdf:application/pdf;ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/XKPZD28J/Thayer and Lane - 2000 - A model of neurovisceral integration in emotion re.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/26X6JU4U/S0165032700003384.html:text/html;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/XF46MJET/S0165032700003384.html:text/html}
