	author = {Stroebe, Margaret and Schut, Henk},
	urldate = {2015-12-01},
	date = {2015-11-01},
	langid = {english},
	pmid = {26581741},
	keywords = {Health, Interpersonal Relations, Trauma, Family, intragroup processes},
	file = {Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/VAQAWKVS/Stroebe and Schut - 2015 - Family Matters in Bereavement Toward an Integrativ.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/TW5228AB/873.html:text/html}
}

@article{hershfield_leveraging_2015,
