}

@article{lee_oxytocin:_2009,
	title = {Oxytocin: The great facilitator of life},
	volume = {88},
	issn = {0301-0082},
	url = {http://www.sciencedirect.com.laneproxy.stanford.edu/science/article/B6T0R-4W1SRT4-1/2/6ce26d67d011daba52f438de48db89e9},
	doi = {10.1016/j.pneurobio.2009.04.001},
	shorttitle = {Oxytocin},
	abstract = {Oxytocin (Oxt) is a nonapeptide hormone best known for its role in lactation and parturition. Since 1906 when its uterine-contracting properties were described until 50 years later when its sequence was elucidated, research has focused on its peripheral roles in reproduction. Only over the past several decades have researchers focused on what functions Oxt might have in the brain, the subject of this review.
Immunohistochemical studies revealed that magnocellular neurons of the hypothalamic paraventricular and supraoptic nuclei are the neurons of origin for the Oxt released from the posterior pituitary. Smaller cells in various parts of the brain, as well as release from magnocellular dendrites, provide the Oxt responsible for modulating various behaviors at its only identified receptor.
Although Oxt is implicated in a variety of "non-social" behaviors, such as learning, anxiety, feeding and pain perception, it is Oxt's roles in various social behaviors that have come to the fore recently. Oxt is important for social memory and attachment, sexual and maternal behavior, and aggression. Recent work implicates Oxt in human bonding and trust as well. Human disorders characterized by aberrant social interactions, such as autism and schizophrenia, may also involve Oxt expression. Many, if not most, of Oxt's functions, from social interactions (affiliation, aggression) and sexual behavior to eventual parturition, lactation and maternal behavior, may be viewed as specifically facilitating species propagation.},
	pages = {127--151},
	number = {2},
	journaltitle = {Progress in Neurobiology},
	author = {Lee, Heon-Jin and Macbeth, Abbe H. and Pagani, Jerome H. and Scott Young 3rd, W.},
	urldate = {2010-06-28},
	date = {2009-06},
	keywords = {Aggression, Anxiety, Autism, Bonding, Schizophrenia, Sexual behavior, Social recognition, Stress, Vasopressin},
	file = {ScienceDirect Full Text PDF:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/X7RBVUTS/Lee et al. - 2009 - Oxytocin The great facilitator of life.pdf:application/pdf;ScienceDirect Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/FXFWDAAR/science.html:text/html}
}

