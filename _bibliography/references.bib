	langid = {english},
	keywords = {Diseases, Genetics research, \_tablet},
	file = {Polderman_et_al-2015-Meta-analysis_of_the_heritability_of_human_traits_based_on_fifty_years_of_twin.pdf:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/2Z3V6E6S/Polderman_et_al-2015-Meta-analysis_of_the_heritability_of_human_traits_based_on_fifty_years_of_twin.pdf:application/pdf;Snapshot:/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/P97JUFHD/ng.3285.html:text/html}
